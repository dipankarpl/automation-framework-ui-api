<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" parallel="methods">
	<test name="UI-Test">
		<parameter name="environment" value="${environment}" />
		<parameter name="browser" value="${browser}" />
		<parameter name="remote_execution" value="${remote_execution}" />
		<parameter name="headless" value="${headless}" />

		<groups>
			<run>
				<include name="regression"></include>
			</run>
		</groups>

		<classes>
			<class name="com.qa.tests.ui.SignupTest" />
			<class name="com.qa.tests.ui.LoginTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
